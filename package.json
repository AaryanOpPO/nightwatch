{
  "name": "nightwatch",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Nightwatch/nightwatch.git",
  "author": "Tanner Goins <tannergz28@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "lint": "tslint --project . --format stylish && yarn web:lint",
    "api:start": "yarn api:dev",
    "api:dev": "tsc && nodemon --watch 'src/api/src/**/*.ts' --exec 'ts-node' src/api/src/index.ts",
    "api:prod": "tsc && cross-env NODE_ENV=production nodemon --exec ts-node src/api/src/index.ts",
    "api:docs": "cross-env typedoc --out public/docs src --name \"Nightwatch API\" --excludeExternals --mode file",
    "api:test": "nyc mocha --opts \"./mocha.opts\"",
    "bot:start": "yarn bot:dev",
    "bot:dev": "tsc && nodemon --watch 'src/bot/**/*.ts' --exec 'ts-node' src/bot/index.ts",
    "bot:prod": "tsc && cross-env NODE_ENV=production nodemon --exec ts-node src/bot/index.ts",
    "web:start": "yarn web:dev",
    "web:dev": "vue-cli-service serve src/web/main.js",
    "web:prod": "vue-cli-service build src/web/main.js",
    "web:lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "^0.0.40",
    "@types/express-rate-limit": "^2.9.3",
    "@types/helmet": "^0.0.42",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.2",
    "@types/sinon": "^5.0.5",
    "@types/socket.io": "^1.4.38",
    "@types/supertest": "^2.0.6",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "husky": "^1.1.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.4",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "discord.js": "discordjs/discord.js",
    "discord.js-commando": "discordjs/commando",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^3.2.1",
    "express-status-monitor": "^1.2.3",
    "helmet": "^3.14.0",
    "inversify": "~4.13.0",
    "inversify-express-utils": "^6.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongo-morgan": "^2.0.3",
    "morgan": "^1.9.1",
    "node-opus": "^0.3.1",
    "nodemon": "^1.18.4",
    "pg": "^7.5.0",
    "ramda": "^0.25.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "socket.io": "^2.1.1",
    "typeorm": "^0.2.8",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}